{
  "env": {
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": ["airbnb/base"],
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "plugins": [
    "import"
  ],
  "rules": {
    "generator-star-spacing": 0,
    "no-duplicate-imports": 0,
    "import/no-duplicates": 2,
    "no-underscore-dangle": 0,
    "no-console": "warn"
  }
}
